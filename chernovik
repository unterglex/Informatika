from random import seed, randint
seed(1013)
x1=randint(1,18)
x2=randint(1,13)
x3=randint(1,14)
print(x1,x2,x3)
16 13 5

'Zadanie 4'
a=[[2,4,3],[3,6,5,2,1,1]]
mini=a[0][0]
stroka=0
stolbec=0
for i in range(3):
    for j in range(6):
        if mini>a[i][j]:
            mini=a[i][j]
            stroka=i
            stolbec=j
print(stroka, stolbec)
